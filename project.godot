config_version=5

[application]

config/name="Orbattle"
run/main_scene="res://screens/battle_screen/battle_screen_scene.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://textures/icon.svg"

[autoload]

MouseMaster="*res://globals/mouse_master.tscn"
TimeMaster="*res://globals/time_master.tscn"
SceneTreeDetails="*res://globals/scene_tree_details.gd"

[debug]

gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/codecapture/plugin.cfg")

[file_customization]

folder_colors={
"res://cards/": "pink",
"res://globals/": "gray",
"res://mechanisms/": "gray",
"res://mechanisms/states/": "gray",
"res://summons/": "orange"
}

